{
  "schemaVersion": 1,
  "id": "valkyrienmanager",
  "version": "${version}",
  "name": "Valkyrien Skies Manager",
  "description": "Makes your life easier with lots of ships. Fire Ship events to kjs, computer craft, query ship data extension through api",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/valkyrienmanager/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.verr1.valkyrienmanager.fabric.ValkyrienManagerModFabric"
    ],
    "client": [
      "com.verr1.valkyrienmanager.fabric.client.ExampleModFabricClient"
    ]
  },
  "mixins": [
    "valkyrienmanager.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.10",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "architectury": ">=9.2.14",
    "fabric-api": "*"
